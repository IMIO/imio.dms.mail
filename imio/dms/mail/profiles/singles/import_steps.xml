<?xml version="1.0"?>
<import-steps>

 <import-step
    id="imiodmsmail-create-templates"
    handler="imio.dms.mail.setuphandlers.create_templates_step"
    title="Templates creation step for imio.dms.mail"
    version="2017-06-22">
    Create templates. If it already exists, just apply the transition and set at right place.
 </import-step>

 <import-step
    id="imiodmsmail-update-templates"
    handler="imio.dms.mail.setuphandlers.update_templates_step"
    title="Templates update step for imio.dms.mail"
    version="2017-06-22">
    Update template files from the os. Don't replace a modified template !
 </import-step>

 <import-step
    id="imiodmsmail-override-templates"
    handler="imio.dms.mail.setuphandlers.override_templates_step"
    title="Templates override step for imio.dms.mail"
    version="2017-06-22">
    Update and override template files from the os. Replace a modified template !
 </import-step>

 <import-step
    id="imiodmsmail-create-persons-from-users"
    handler="imio.dms.mail.setuphandlers.create_persons_from_users_step"
    title="Create persons from users"
    version="2017-06-22">
    For internal people. Create persons and held positions in own personal folder.
 </import-step>

 <import-step
    id="imiodmsmail-create-persons-from-users-inverted"
    handler="imio.dms.mail.setuphandlers.create_persons_from_users_step_inverted"
    title="Create persons from users, fullname starting with lastname"
    version="2017-06-22">
    For internal people. Create persons and held positions in own personal folder.
 </import-step>

 <import-step
    id="imiodmsmail-add-icons-to-contact-workflow"
    handler="imio.dms.mail.setuphandlers.add_icons_to_contact_workflow"
    title="Add transition icons to collective_contact_core_workflow."
    version="2018-05-17">
    Add transition icons to collective_contact_core_workflow.
 </import-step>

</import-steps>
